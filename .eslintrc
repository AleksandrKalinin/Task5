{
  "parser": "@babel/eslint-parser",
  "rules": {
    "eqeqeq": 2,
    "no-empty-function": 1,
    "no-var": 1,
    "prefer-const": 1,
    "no-console": 2,
    "no-unused-vars": 0,
    "no-duplicate-imports": 1,
    "max-len": [1, 100, 2, {ignoreComments: true}],
    "brace-style": 1,
    "indent": ["error", 2],
    "no-multiple-empty-lines": 1,
    quotes: ["error", "single"]
  }
}